type User {
  id: ID! @id
  name: String!
  email: String! @unique
  username: String! @unique
  password: String!
  createdAt: DateTime! @createdAt
}

type Student {
  id: ID! @id
  profileImg: String
  name: String!
  gender: GenderType!
  birthDate: DateTime!
  email: String!
}

enum GenderType {
  MALE
  FEMALE
  RATHER_NOT_SAY
}